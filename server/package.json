{
  "name": "pubg-tournament-tracker-server",
  "version": "1.0.1",
  "description": "Backend server for PUBG Tournament Tracker",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "knex": "knex --knexfile src/db/knexfile.js",
    "migrate": "npm run knex migrate:latest",
    "migrate:make": "npm run knex migrate:make",
    "seed": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make",
    "migrate:sql": "node ./src/db/runMigrations.js",
    "db:fix": "npm run migrate:sql",
    "test": "jest"
  },
  "keywords": [
    "pubg",
    "tournament",
    "api",
    "nodejs",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "recharts": "^2.9.0",
    "redis": "^4.6.8",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
